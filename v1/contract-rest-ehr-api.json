{
  "openapi": "3.0.2",
  "info": {
    "title": "EHR Data Authority",
    "description": "OpenAPI 3.0 specification for the EHR Data Authority",
    "contact": {
      "name": "Josh Smith",
      "email": "josh@projectronin.com",
      "url": "https://github.com/projectronin/contract-rest-emr-api"
    },
    "version": "1.0.0-SNAPSHOT"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "tags": [
    {
      "name": "resources",
      "description": "Relating to FHIR resources"
    }
  ],
  "paths": {
    "/resources": {
      "post": {
        "tags": [
          "resources"
        ],
        "summary": "Adds or updates the supplied resource",
        "description": "Adds or updates the supplied resource",
        "operationId": "addResource",
        "requestBody": {
          "description": "The resource to add or update in the EHR Data Authority",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Resource"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "modificationType": {
                      "$ref": "#/components/schemas/ModificationType"
                    }
                  }
                }
              }
            }
          },
          "default": {
            "description": "An error occurred while attempting to add the resource",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/resources/batch": {
      "post": {
        "tags": [
          "resources"
        ],
        "summary": "Adds or updates the supplied resources",
        "description": "Adds of updates the supplied resources. A maximum of 25 resources are allowed in a single request.",
        "operationId": "addResources",
        "requestBody": {
          "description": "The resources to add or update in the EHR Data Authority",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/Resource"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Request processed successfully. Consult the response for individual resource success and failure details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BatchResourceResponse"
                }
              }
            }
          },
          "default": {
            "description": "An error occurred while attempting to add the resource",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/tenants/{tenantMnemonic}/resources/{resourceType}/{udpId}": {
      "get": {
        "tags": [
          "resources"
        ],
        "parameters": [
            {
            "name": "udpId",
            "in": "path",
            "description": "UDP Id of resource to return",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tenantMnemonic",
            "in": "path",
            "description": "The mnemonic that identifies the tenant",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "resourceType",
            "in": "path",
            "description": "The resource type to search for, can be any resource",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "summary": "Returns a resource",
        "description": "Returns the resource defined by the given UDP Id",
        "operationId": "getResource",
        "responses": {
          "200": {
            "description": "Request processed successfully, resource returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Resource"
                }
              }
            }
          },
          "default": {
            "description": "An error occurred while attempting to get the resource",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/tenants/{tenantMnemonic}/resources/{resourceType}/identifiers/{identifier}": {
      "get": {
        "tags": [
          "resources"
        ],
        "parameters": [
          {
            "name": "identifier",
            "in": "path",
            "description": "Array of identifiers used to search for resources.  Identifiers in the format 'system|value'.",
            "required": true,
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "minItems": 1
            }
          },
          {
            "name": "resourceType",
            "in": "path",
            "description": "The resource type to search for",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/IdentifierSearchableResourceTypes"
            }
          },
          {
            "name": "tenantMnemonic",
            "in": "path",
            "description": "The mnemonic that identifies the tenant",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "summary": "Returns a list of identifiers",
        "description": "Returns all identifiers tied to a resource indicated by the given identifier",
        "operationId": "getResourceIdentifiers",
        "responses": {
          "200": {
            "description": "Request processed successfully, identifiers returned",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IdentifierSearchResponse"
                }
              }
            }
          },
          "default": {
            "description": "An error occurred while attempting to find the identifiers",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
