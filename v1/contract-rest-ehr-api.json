            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "resourceType": {
                  "type": "string",
                  "description": "The type of the resource"
                },
                "resourceId": {
                  "type": "string",
                  "description": "The ID of the resource"
                },
                "error": {
